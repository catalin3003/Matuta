<?xml version='1.0' encoding='utf-8'?>
<widget id="com.zincmedia.formulaforthrillsportugal" version="1.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0" xmlns:gap="http://phonegap.com/ns/1.0">
    <feature name="Camera">
        <param name="browser-package" value="Camera" />
    </feature>
    <feature name="Device">
        <param name="browser-package" value="Device" />
    </feature>
    <name>Thrills</name>
    <description>
        Siemens Formula for Thrills Portugal
    </description>
    <author email="catalin@redhearer.com" href="http://zincmedia.com">
        Zinc Media
    </author>
    <content src="index.html" />
    <icon src="res/ios/icon-72-2x.png" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <preference name="DisallowOverscroll" value="true" />
    <preference name="Fullscreen" value="true" />
    <preference name="BackgroundColor" value="0xff0000ff" />
    <preference name="Orientation" value="landscape" />
    <preference name="android-minSdkVersion" value="14" />
</widget>
