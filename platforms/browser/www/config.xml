<?xml version='1.0' encoding='utf-8'?>
<widget id="com.zincmedia.formulaforthrillsportugal" version="1.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0" xmlns:gap="http://phonegap.com/ns/1.0">
    <feature name="Camera">
        <param name="browser-package" value="Camera" />
    </feature>
    <feature name="Device">
        <param name="browser-package" value="Device" />
    </feature>
    <name>Matuta</name>
    <description>Matuta - Siemens Formula for Thrills Portugal
    </description>
    <author email="cat@catcodes.com" href="http://catcodes.com">
        Catalin Magirescu
    </author>
    <content src="index.html" />
    <icon gap:platform="ios" height="57" src="res/icon-57.png" width="57" />
    <icon gap:platform="ios" height="72" src="res/icon-72.png" width="72" />
    <icon gap:platform="ios" height="114" src="res/icon-114.png" width="114" />
    <icon gap:platform="ios" height="120" src="res/icon-120.png" width="120" />
    <icon gap:platform="ios" height="144" src="res/icon-144.png" width="144" />
    <icon gap:platform="ios" height="152" src="res/icon-152.png" width="152" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <preference name="DisallowOverscroll" value="true" />
    <preference name="Fullscreen" value="true" />
    <preference name="BackgroundColor" value="0xff0000ff" />
    <preference name="Orientation" value="landscape" />
    <preference name="android-minSdkVersion" value="14" />
</widget>
