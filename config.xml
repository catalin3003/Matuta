<?xml version='1.0' encoding='utf-8'?>
<widget id="com.zincmedia.formulaforthrillsportugal" version="1.1" xmlns="http://www.w3.org/ns/widgets" xmlns:gap="http://phonegap.com/ns/1.0">
    <name>Matuta</name>
    <description>Matuta - Siemens Formula for Thrills Portugal
    </description>
    <author email="cat@catcodes.com" href="http://catcodes.com">
        Catalin Magirescu
    </author>
    <content src="index.html" />
    <icon src="res/icon-57.png" gap:platform="ios" width="57" height="57" />
    <icon src="res/icon-72.png" gap:platform="ios" width="72" height="72" />
    <icon src="res/icon-114.png" gap:platform="ios" width="114" height="114" />
    <icon src="res/icon-120.png" gap:platform="ios" width="120" height="120" />
    <icon src="res/icon-144.png" gap:platform="ios" width="144" height="144" />
    <icon src="res/icon-152.png" gap:platform="ios" width="152" height="152" />
    <preference name="DisallowOverscroll" value="true"/>
    <preference name="Fullscreen" value="true" />
    <preference name="BackgroundColor" value="0xff0000ff"/>
    <preference name="Orientation" value="landscape" />
    <preference name="android-minSdkVersion" value="14" />
    <plugin name="cordova-plugin-device" source="npm" spec="~1.1.1" />
    <plugin name="cordova-plugin-device-orientation" source="npm" spec="~1.0.2" />
    <plugin name="cordova-plugin-inappbrowser" source="npm" spec="~1.3.0" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
</widget>